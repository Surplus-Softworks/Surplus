{
  "manifest_version": 2,
  "name": "Surplus",
  "description": "A cheat for survev.io & more",
  "version": "%VERSION%",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "contact@s.urpl.us",
      "strict_min_version": "109.0"
    }
  }
}